    "express-mongo-sanitize": "^2.0.1",
    "express-rate-limit": "^5.2.3",
    "html-to-text": "^7.0.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.13.5",
    "mongoose-auto-increment": "^5.0.1",
    "multer": "^1.4.2",
    "nodemailer": "^6.4.16",
    "nodemon": "^2.0.6",
